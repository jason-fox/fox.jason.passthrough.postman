<project name="fox.jason.passthrough.postman" default="postman.process" xmlns:postman="antlib:fox.jason.passthrough.postman">
	<!--
		Iterate across all files marked format="postman" and process them
	-->
	<target name="postman.process">
		<property name="passthrough.input" value=""/>
		<property name="passthrough.output" value=""/>
		<postman:add-file src="${passthrough.input}" dest="${passthrough.output}"/>
	</target>
</project>