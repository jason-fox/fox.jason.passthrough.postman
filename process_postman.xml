<project name="fox.jason.passthrough.postman" default="postman.process">
	<!--
		Iterate across all files marked format="postman" and process them
	-->
	<target name="postman.process">
		<property name="passthrough.input" value=""/>
		<property name="passthrough.output" value=""/>
		<taskdef classname="fox.jason.passthrough.tasks.PostmanToMarkdownTask" name="postman-to-markdown" onerror="ignore">
			<classpath>
				<fileset dir="lib">
					<include name="**/*.jar"/>
				</fileset>
			</classpath>
		</taskdef>
		<postman-to-markdown src="${passthrough.input}" dest="${passthrough.output}"/>
	</target>
</project>